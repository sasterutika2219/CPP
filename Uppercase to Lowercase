//Convert upper case to lower case

#include<iostream>
#include<string>
using namespace std;

int main(){
    string s="WeLcOme";
    // for(int i=0;s[i]!='\0';i++){
    //     if(s[i]>=65 && s[i]<=90){
    //         s[i]=s[i]+32;
    //     }        
    // }

    string::iterator it;
    for(it=s.begin();it!=s.end();it++){
        if(*it>=65 && *it<=90){
            *it=*it+32;
        }
    }
    cout<<s;
}
